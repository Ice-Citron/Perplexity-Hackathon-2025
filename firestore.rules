rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read/write for analysis results cache
    match /analyses/{analysisId} {
      allow read: if true;
      allow write: if true;
    }

    // Allow read/write for logs
    match /logs/{logId} {
      allow read: if true;
      allow write: if true;
    }
  }
}
